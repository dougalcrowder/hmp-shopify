{%- assign show_payment_icons = false -%}

{%- if section.settings.show_payment_icons -%}
  {%- unless shop.enabled_payment_types == empty -%}
    {%- assign show_payment_icons = true -%}
  {%- endunless -%}
{%- endif -%}

{%- capture payment_icon_markup -%}
  {%- if show_payment_icons -%}
    <div class="payment-icons inner">
      <span class="visually-hidden">{{ 'general.payment.method' | t }}</span>
      <ul class="payment-icons flex">
        {%- for type in shop.enabled_payment_types -%}
          <li class="payment-icon">
            {{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}
          </li>
        {%- endfor -%}
      </ul>
    </div>
  {%- endif -%}
{%- endcapture -%}

<footer class="site-footer" role="contentinfo" data-section-id="{{ section.id }}" data-section-type="footer-section">
  <div class="page-width site_footer inner grid">
    {% comment %}
    <div class="flex nlogo">
      <span class="footer_logo">
        <a href="/" alt="go to the homepage">
          <span class="visually-hidden">{{ shop.title }}</span>
        </a>
      </span>
    </div>
    {% endcomment %}
    <div class="flex nuls">
      {%- for block in section.blocks -%}
          <div class="footer_item footer-{{ block.type }}">

            {%- case block.type -%}
              {%- when 'newsletter' -%}
              
              {%- if block.settings.title -%}
              <strong class="product">{{ block.settings.title | escape }}</strong>
              {%- endif -%}
              
                <div class="site-footer__newsletter">
                  {% include 'newsletter_footer' %}
                  {%- assign socials = "Facebook Twitter Pinterest Instagram Tumblr Snapchat YouTube Vimeo" | split: ' ' -%}
                  <div class="social">
                      <ul class="social-icons flex">
                        {%- for social in socials -%}
                          {%- assign social_link = social | prepend: 'social_' | append: '_link' | downcase -%}
                          {%- assign icon_link = social | prepend: 'icon-' | downcase -%}
                          {%- if settings[social_link] != blank -%}
                            <li class="social-icon">
                              <a class="social-icon__link" href="{{ settings[social_link] | escape }}" aria-describedby="a11y-external-message" alt="{{ social }}">
                                {%- include icon_link -%}
                              </a>
                            </li>
                          {%- endif -%}
                        {%- endfor -%}
                      </ul>
                  </div>
                </div>

              {%- when 'text' -%}
                {%- if block.settings.title -%}
                  <strong>{{ block.settings.title | escape }}</strong>
                {%- endif -%}
                {%- if block.settings.text != blank -%}
                  <div class="site-footer__rte">
                      {{ block.settings.text }}
                  </div>
                {%- endif -%}

              {%- when 'link_list' -%}
                {%- if block.settings.title -%}
                  <strong class="product">{{ block.settings.title | escape }}</strong>
                {%- endif -%}
                {%- assign footer_linklist = block.settings.menu -%}
                  <ul class="footer_linklist
                            {% if inline_list %}list--inline{% endif %}">
                    {%- for link in linklists[footer_linklist].links -%}
                      <li class="footer_linklist-item">
                        <a href="{{ link.url }}"
                          {% if link.current %} aria-current="page"{% endif %}
                        >
                          {{ link.title | escape }}
                        </a>
                      </li>
                    {%- endfor -%}
                  </ul>

            {%- endcase -%}

          </div>

      {%- endfor -%}
    </div>
    
  </div>



  <div class="page-width footer_bar">
    {{ payment_icon_markup }}
    <div class="flex">
      <div class="copyright">
        <small class="site-footer__copyright-content">&copy; {{ 'now' | date: "%Y" }}, {{ shop.name | link_to: routes.root_url }}. All Rights Reserved. Company registration number 09830217</small>
      </div>
    </div>
  </div>
</footer>



